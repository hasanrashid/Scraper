{
    "Download Link RegEx":"mediafire\\.com|drive\\.google\\.com|datafilehost\\.com|goo\\.gl|bit\\.ly",
    "Download URL":{
        "drive.google.com":{
            "URL": "https://drive.google.com/uc",
            "File ID regex":"(?:id=|d\/)(?P<id>[a-zA-Z-0-9]*)",
            "Request Params":{
                "export":"download"
            },
            "Cookie":"download_warning"
        },
        "www.datafilehost.com":{
            "URL":"http://www.datafilehost.com/get.php",
            "File ID regex": "d\/(?P<file>[0-9A-Za-z]*)",
            "Cookie":"PHPSESSID"
        },        
        "goo.gl":{
            "action":"download"
        },
        "bit.ly":{
            "action":"download"
        },
        "www.mediafire.com":{
            "URL":"#download_link",
            "action":"download"
        }
    }   
}

