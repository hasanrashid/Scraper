{
    "Download Link RegEx":"mediafire\\.com|drive\\.google\\.com|datafilehost\\.com|goo\\.gl|bit\\.ly",
    "Download URL":{
        "drive.google.com":{
            "action": "construct",
            "First URL": "https://drive.google.com/uc",
            "Second URL":"https://drive.google.com/uc",
            "Request Params":[
                {
                    "name": "id",
                    "type":"regex",
                    "value": "(?:id=|d\/)([a-zA-Z-0-9]*)"
                },
                {
                    "name": "export",
                    "type":"text",
                    "value": "download"
                }
            ],
            "Cookies":[
                {
                    "name":"download_warning_",
                    "action": "read",
                    "value": "confirm"                    
                }
            ]
        },
        "www.datafilehost.com":{
            "action": "construct",
            "First URL":"unchanged",
            "Second URL":"http://www.datafilehost.com/get.php",
            "Request Params":[
                {
                    "name": "file",
                    "type":"regex",
                    "value": "d\/([0-9A-Za-z]*)"
                }
            ],
            "Cookies":[
                {
                    "name":"PHPSESSID",
                    "value":{
                        "PHPSESSID":"{}"
                    },
                    "action": "attach"    
                }
            ]
        },        
        "goo.gl":{
            "action":"download"
        },
        "bit.ly":{
            "action":"download"
        },
        "www.mediafire.com":{
            "action":"download"
        }
    }   
}

